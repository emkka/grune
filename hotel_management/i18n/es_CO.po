#: model:mail.template,body_html:hotel_management.hotel_reservation_confirmed_mail_template
msgid ""
"<div style=\"margin: 0px; padding: 0px;\">\n"
"                <p style=\"margin: 0px; padding: 0px; font-size: 12px;\">\n"
"        Hello,\n"
"                    <br/>\n"
"                    <br/>\n"
"        Your reservation order\n"
"                    <strong>${object.reservation_no}</strong> amounting in\n"
"                    <strong>${format_amount(object.total_cost1, object.currency_id)}</strong>\n"
"            has been confirmed.\n"
"                    <br/>\n"
"            Thank you for your trust!\n"
"                    <br/>\n"
"                    <br/>\n"
"        Do not hesitate to contact us if you have any questions.\n"
"                    <br/>\n"
"                    <br/>\n"
"                </p>\n"
"                <div style=\"margin: 0px; padding: 0px;\">\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"border-bottom: 2px solid #dee2e6;\">\n"
"                            <td style=\"width: 150px;\">\n"
"                                <strong>Room</strong>\n"
"                            </td>\n"
"                            <td width=\"15%\" align=\"center\">\n"
"                                <strong>No. of days</strong>\n"
"                            </td>\n"
"                            <td width=\"20%\" align=\"right\">\n"
"                                <strong>\n"
"                                Price\n"
"                </strong>\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"                    <br/>\n"
"        % for line in object.reservation_line:\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"${loop.cycle('background-color: #f2f2f2', 'background-color: #ffffff')}\">\n"
"                            <td align=\"left\">${line.room_number.name}</td>\n"
"                            <td width=\"15%\" align=\"center\">${line.number_of_days}</td>\n"
"                            <td width=\"20%\" align=\"right\">\n"
"                                <strong>\n"
"                                    ${format_amount(line.sub_total1, object.currency_id)}\n"
"                                </strong>\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"        % endfor\n"
"                </div>\n"
"                <br/>\n"
"                <br/>\n"
"                <div style=\"margin: 0px; padding: 0px;\">\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"border-bottom: 2px solid #dee2e6;\">\n"
"                            <td style=\"width: 150px;\">\n"
"                                <strong>Product</strong>\n"
"                            </td>\n"
"                            <td width=\"15%\" align=\"center\">\n"
"                                <strong>Quantity</strong>\n"
"                            </td>\n"
"                            <td width=\"20%\" align=\"right\">\n"
"                                <strong>\n"
"                                Price\n"
"                </strong>\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"                    <br/>\n"
"        % for line in object.other_items_ids:\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-collapse: collapse;\">\n"
"                        <tr style=\"${loop.cycle('background-color: #f2f2f2', 'background-color: #ffffff')}\">\n"
"                            <td align=\"left\">${line.product_id.name}</td>\n"
"                            <td width=\"15%\" align=\"center\">${line.product_uom_qty}</td>\n"
"                            <td width=\"20%\" align=\"right\">\n"
"                                <strong>\n"
"                                    ${format_amount(line.price_subtotal, object.currency_id)}\n"
"                                </strong>\n"
"                            </td>\n"
"                        </tr>\n"
"                    </table>\n"
"        % endfor\n"
"                </div>\n"
"                <br/>\n"
"                <div style=\"margin: 0px; padding: 0px;\">\n"
"                    <table width=\"100%\" style=\"color: #454748; font-size: 12px; border-spacing: 0px 4px;\" align=\"right\">\n"
"                        <tr>\n"
"                            <td style=\"width: 60%\"/>\n"
"                            <td style=\"width: 30%; border-top: 1px solid #dee2e6;\" align=\"right\">\n"
"                                <strong>Subtotal:</strong>\n"
"                            </td>\n"
"                            <td style=\"width: 10%; border-top: 1px solid #dee2e6;\" align=\"right\">${format_amount(object.untaxed_amt, object.currency_id)}</td>\n"
"                        </tr>\n"
"                        <tr>\n"
"                            <td style=\"width: 60%\"/>\n"
"                            <td style=\"width: 30%;\" align=\"right\">\n"
"                                <strong>Tax:</strong>\n"
"                            </td>\n"
"                            <td style=\"width: 10%;\" align=\"right\">${format_amount(object.total_tax, object.currency_id)}</td>\n"
"                        </tr>\n"
"                        <tr>\n"
"                            <td style=\"width: 60%\"/>\n"
"                            <td style=\"width: 30%;\" align=\"right\">\n"
"                                <strong>Total:</strong>\n"
"                            </td>\n"
"                            <td style=\"width: 10%;\" align=\"right\">${format_amount(object.total_cost1, object.currency_id)}</td>\n"
"                        </tr>\n"
"                    </table>\n"
"                </div>\n"
"            </div>\n"
"        "
